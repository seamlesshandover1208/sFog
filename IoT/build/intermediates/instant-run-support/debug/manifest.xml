            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.androidtemplate.DescActivity" />
        <activity android:name="com.example.androidtemplate.SocketActivity" />

        <provider
